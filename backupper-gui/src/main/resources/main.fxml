<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.Tab?>
<?import java.net.URL?>

<?import pl.gda.pg.eti.autyzm.Strings?>
<BorderPane styleClass="root" id="root" fx:id="root"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.gda.pg.eti.autyzm.Controller.MainController">
    <center>
        <TabPane>
            <Tab closable="false">
                <text><Strings fx:constant="MAKE_BACKUP"/></text>
                <fx:include source="backup.fxml"/>
            </Tab>
            <Tab closable="false">
                <text><Strings fx:constant="REFRESH_COPY"/></text>
                <fx:include source="restore.fxml"/>
            </Tab>
        </TabPane>
    </center>

    <stylesheets>
        <URL value="@main.css" />
    </stylesheets>

</BorderPane>
